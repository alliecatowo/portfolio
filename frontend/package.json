{
    "name": "portfolio-frontend",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "22.x"
    },
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "node -e \"try { require('nuxt').prepare() } catch (e) { console.warn('Nuxt prepare failed, continuing anyway:', e) }\"",
        "vercel-build": "node install-deps.js && ROLLUP_SKIP_NATIVES=true npm install --force && nuxt build"
    },
    "overrides": {
        "rollup": "3.29.4"
    },
    "dependencies": {
        "@directus/sdk": "^19.0.1",
        "@nuxt/image": "^1.9.0",
        "@nuxtjs/color-mode": "^3.5.2",
        "@nuxtjs/strapi": "^1.9.3",
        "@pinia/nuxt": "^0.5.1",
        "pinia": "^2.1.7",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@nuxt/devtools": "latest",
        "@nuxtjs/tailwindcss": "^6.8.0",
        "nuxt": "^3.8.0"
    }
}